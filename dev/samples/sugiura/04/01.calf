<cauliflower version="1.0">
  <html>&lt;!DOCTYPE html&gt;
&lt;html&gt;
	&lt;head&gt;
		&lt;meta charset="UTF-8"/&gt;
		&lt;title&gt;03-1&lt;/title&gt;
	&lt;/head&gt;
	&lt;body&gt;
		&lt;form&gt;
			数値を入力してください: 
			&lt;input id="number" size="10" type="text"&gt;
			&lt;input value="OK" type="button" onclick="changeFormColor()"&gt;
		&lt;/form&gt;
	&lt;/body&gt;
&lt;/html&gt;
  
  </html>
  <blocks>
    <block type="procedures_defnoreturn" x="28" y="19">
      <mutation></mutation>
      <title name="NAME">changeFormColor</title>
      <statement name="STACK">
        <block type="variables_set" inline="false">
          <title name="VAR">num</title>
          <value name="VALUE">
            <block type="math_parse" inline="true">
              <title name="TYPE">INT</title>
              <value name="VALUE">
                <block type="myDocument_get">
                  <title name="TARGET">number</title>
                  <title name="ATTRIBUTE">.value</title>
                </block>
              </value>
            </block>
          </value>
          <next>
            <block type="controls_if" inline="false">
              <value name="IF0">
                <block type="logic_compare" inline="true">
                  <title name="OP">EQ</title>
                  <value name="A">
                    <block type="variables_get">
                      <title name="VAR">num</title>
                    </block>
                  </value>
                  <value name="B">
                    <block type="math_number">
                      <title name="NUM">2</title>
                    </block>
                  </value>
                </block>
              </value>
              <statement name="DO0">
                <block type="myDocument_set" inline="false">
                  <title name="TARGET">number</title>
                  <title name="ACTION">.style.backgroundColor</title>
                  <value name="VALUE">
                    <block type="text">
                      <title name="TEXT">blue</title>
                    </block>
                  </value>
                </block>
              </statement>
            </block>
          </next>
        </block>
      </statement>
    </block>
  </blocks>
</cauliflower>