                      <title name="TEXT">こんにちは、</title>
                    </block>
                  </value>
                  <value name="ADD1">
                    <block type="variables_get">
                      <title name="VAR">name</title>
                    </block>
                  </value>
                  <value name="ADD2">
                    <block type="text">
                      <title name="TEXT">さん</title>
                    </block>
                  </value>
                </block>
              </value>
            </block>
          </next>
        </block>
