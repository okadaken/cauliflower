		&lt;title&gt;03-4&lt;/title&gt;
	&lt;/head&gt;
	&lt;body&gt;
		&lt;form&gt;
			お名前をどうぞ: 
			&lt;input id="input_name" size="20" type="text"&gt;
			&lt;input value="OK" onclick="hello()" type="button"&gt;
		&lt;/form&gt;
	&lt;/body&gt;
&lt;/html&gt;
  
  </html>
  <blocks>
    <block type="procedures_defnoreturn" x="-2" y="2">
      <mutation></mutation>
      <title name="NAME">hello</title>
      <statement name="STACK">
        <block type="variables_set" inline="false">
          <title name="VAR">name</title>
          <value name="VALUE">
            <block type="myDocument_get">
              <title name="TARGET">input_name</title>
              <title name="ATTRIBUTE">.value</title>
